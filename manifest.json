{
  "manifest_version": 3,
  "name": "Glass Tab Dashboard",
  "version": "1.0.0",
  "description": "A glassmorphism new tab with clock, weather, shortcuts, focus timer, and custom background.",
  "permissions": ["topSites"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "action": {
  "default_title": "Glass Tab Dashboard"
},
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
